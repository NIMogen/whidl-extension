{
	"compilerOptions": {
		"module": "commonjs",
		"target": "es2021",
		"outDir": "out",
		"lib": [
			"es2021"
		],
		"esModuleInterop": true,
		"sourceMap": true,
		"rootDir": "src",
		"strict": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"noImplicitReturns": true,
		"noFallthroughCasesInSwitch": true,
	},
}
